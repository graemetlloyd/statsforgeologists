AmmoniteDiameters <- c(
rep(62.5, 3), #51tp75
rep(87.5, 2), #76tp100
rep(112.5, 15), #101to125
rep(137.5, 23), #126to150
rep(162.5, 43), #151to175
rep(187.5, 25), #176to200
rep(212.5, 13), #201to225
rep(237.5, 5), #226to250
rep(262.5, 1) #251to275
)
length(AmmoniteDiameters)
hist(AmmoniteDiameters, breaks = c(50, 75, 100, 125, 150, 175, 200, 225, 250, 275), border = 0, col = "Grey", xlab = "Ammonite Diameter (mm)", main = paste("Histogram of Lower Bed ammonite diameters\n(Mean* = ", round(mean(AmmoniteDiameters), 2), ", SD* = ", round(sd(AmmoniteDiameters), 2), ")", sep = ""))
FittedNormal <- dnorm(x = seq(from = 50, to = 275, length.out = 1000), mean = mean(AmmoniteDiameters), sd = sd(AmmoniteDiameters))
points(x = seq(from = 50, to = 275, length.out = 1000), y = FittedNormal * length(AmmoniteDiameters) * 25, type = "l", col = "Red", lwd = 3)
